{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
	  "template": "d",
          "foreground": "#0087ff",
          "background": "transparent",
          "type": "text",
          "style": "plain"
	},
	{
          "template": "e",
          "foreground": "#00afff",
          "background": "transparent",
          "type": "text",
          "style": "plain"
        },
        {
          "template": "v",
          "foreground": "#00afd7",
          "background": "transparent",
          "type": "text",
          "style": "plain"
        },
        {
          "template": "i",
          "foreground": "#00d7d7",
          "background": "transparent",
          "type": "text",
          "style": "plain"
        },
        {
          "template": "n",
          "foreground": "#00d7d7",
          "background": "transparent",
          "type": "text",
          "style": "plain"
        },
        {
          "template": "k",
          "foreground": "#00ffd7",
          "background": "transparent",
          "type": "text",
          "style": "plain"
        },
        {
          "template": "l",
          "foreground": "#00d7af",
          "background": "transparent",
          "type": "text",
          "style": "plain"
        },
        {
          "template": "ep",
          "foreground": "#00d7d7",
          "background": "transparent",
          "type": "text",
          "style": "plain"
        },
        {
          "template": "p",
          "foreground": "#00afd7",
          "background": "transparent",
          "type": "text",
          "style": "plain"
        },
	{
          "properties": {
            "style": "full"
          },
          "foreground": "#af00ff",
          "background": "transparent",
          "type": "path",
          "style": "plain"
        },
        {
          "template": "> ",
          "foreground": "white",
          "background": "transparent",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3
}
